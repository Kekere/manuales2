<!-- container section start -->
<section id="main-content">
<div class="container">
  <a href="/agregar" class="btn btn-success">Agregar</a><hr>
  <table class="table table-striped">
      <thead>
          <tr>
              <th>ID</th>
              <th>NOMBRE</th>
              <th>DESCRIPCIÓN</th>
              <th>PRECIO</th>
              <th>CANTIDAD</th>
              <th>AUTOR</th>
              <th>TECNOLOGÍA</th>
              <th>ARCHIVO </th>
              <th colspan="2" width="10%">ACCIÓN</th>
          </tr>
      </thead>
      <%for(var i=0; i<productos.length;i++){%>
      <tbody>
       
          <tr>
              <td><%=productos[i].id%></td>
              <td><%=productos[i].nombre%></td>
              <td><%=productos[i].descripcion%></td>
              <td><%=productos[i].precio%></td>
              <td><%=productos[i].cantidad%></td>
              <td><%=productos[i].autor%></td>
              <td><%=productos[i].tecnologia%></td>
              <td><%=productos[i].archivo%></td>
              <td>
                  <a href="/editar/<%= productos[i].id %>" class="btn btn-warning">Editar</a>
              </td>
              <td>
               <a href="/delete/<%= productos[i].id %>" class="btn btn-danger">Eliminar</a>
              </td>
          </tr>
        
      </tbody>
      <% } %> 
  </table>
  </div>
</section>

