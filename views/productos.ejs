<%- include layouts/header.ejs %>

    <!-- banner -->
    <div class="page-head">
        <div class="container">
            <h3>Nuestros Manuales</h3>
        </div>
    </div>
    <!-- //banner -->
    <!-- Electronics -->
    <div class="electronics">
        <div class="container">
            <div class="col-md-8 electro-left text-center">
                <div class="clearfix"></div>
            </div>

            <div class="clearfix"></div>
            <div class="ele-bottom-grid">
                <h3><span>Recientes </span></h3>
                <%for(var i=0; i<productos.length; i++){%>


                    <div class="col-md-3 product-men ">
                        <div class="men-pro-item  simpleCart_shelfItem">
                            <div class="men-thumb-item">
                                <img src="/images/upload_images/<%= productos[i].imagen %>" class="pro-image-front">
                                <img src="/images/upload_images/<%= productos[i].imagen %>" class="pro-image-back">
                                <input type="hidden" value="<%= productos[i].id %>">
                                <div class="men-cart-pro">
                                    <div class="inner-men-cart-pro">
                                        <a href="/descripcion/<%= productos[i].id %>" class="link-product-add-cart">Ver Descripci√≥n</a>
                                    </div>
                                </div>
                            </div>
                            <div class="item-info-product ">
                                <h4>
                                    <p class="item_name">
                                        <%=productos[i].nombre%>
                                    </p>
                                    <a href="/descripcion/<%= productos[i].id %>">
                                        <%=productos[i].nombre%>
                                    </a>
                                </h4>
                                <div class="info-product-price">
                                    <span class="item_price">$<%=productos[i].precio%></span>
                                    <span class="item_name"><%=productos[i].name%></span>

                                </div>
                                <a href="#" class="item_add single-item hvr-outline-out button2">Agregar al Carrito</a>
                            </div>
                        </div>
                    </div>
                    <% } %>
                        <div class="clearfix"></div>
            </div>
        </div>
    </div>
    <!-- //Electronics -->
    <% include layouts/footer.ejs %>